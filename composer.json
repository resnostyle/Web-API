{
  "name": "laranzedb/web-api",
  "description": "nZEDb Web & API Rewrite using Laravel",
  "keywords": [
    "nZEDb",
    "usenet",
    "laravel"
  ],
  "license": "MIT",
  "type": "project",
  "authors": [
    {
      "name": "Zetas",
      "email": "zetas@zet.as"
    }
  ],
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/LaranZEDb/l5-fixtures"
    },
    {
      "type": "vcs",
      "url": "https://github.com/LaranZEDb/Entrust"
    }
  ],
  "require": {
    "php": ">=7.0.0",
    "eduardokum/laravel-mail-auto-embed": "^1.0",
    "fideloper/proxy": "~3.3",
    "jenssegers/mongodb": "^3.3.0-alpha",
    "laravel/framework": "5.5.*",
    "laravel/tinker": "~1.0",
    "predis/predis": "^1.1",
    "spatie/laravel-fractal": "^5.2",
    "zizaco/entrust": "dev-l5.5-support",
    "mayconbordin/l5-fixtures": "dev-l5.5-support"
  },
  "require-dev": {
    "barryvdh/laravel-ide-helper": "^2.4",
    "behat/behat": "^3.4",
    "behat/mink": "^1.7",
    "behat/mink-extension": "^2.2",
    "doctrine/dbal": "^2.6",
    "filp/whoops": "~2.0",
    "fzaninotto/faker": "~1.4",
    "itsgoingd/clockwork": "^2.0",
    "laracasts/behat-laravel-extension": "^1.0",
    "mockery/mockery": "0.9.*",
    "phpunit/phpunit": "~6.0",
    "squizlabs/php_codesniffer": "*"
  },
  "autoload": {
    "classmap": [
      "database/seeds",
      "database/factories"
    ],
    "psr-4": {
      "App\\": "app/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "dont-discover": [
      ]
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate"
    ],
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover"
    ]
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
